{
  "name": "@beisen/paas-components-template",
  "version": "0.0.1",
  "description": "pas-components-template",
  "main": "lib/index.js",
  "_storybook":{
    "default_example": "man/index.js"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:story",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/index.js --config webpack.config.js",
    "build:story": "babel story --out-dir man",
    "test": "karma start",
    "dev": "webpack-dev-server --progress --colors --config webpack.dev.config.js",
    "renew": "git checkout-index -f -a --prefix=../$name/ && cp ./.gitignore ../$name"
  },
  "keywords": [
    "beisen",
    "react-component",
    "es6",
    "karma",
    "jasmine"
  ],
  "repository": {
    "type": "git",
    "url": "git@gitlab.beisen.co:cnpm/beisen-module-template.git"
  },
  "author": {
    "name": "zhongjiahao",
    "email": "zhongjiahao@beisen.com"
  },
  "maintainers": [
    {
      "name": "zhongjiahao",
      "email": "zhongjiahao@beisen.com"
    }
  ],
  "license": "ISC",
