    "babel-preset-stage-0": "^6.5.0",
    "case-sensitive-paths-webpack-plugin": "^1.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.0.0",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.3.2",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
